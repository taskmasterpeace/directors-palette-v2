{
  "appId": "com.directorspalette.app",
  "appName": "Directors Palette",
  "webDir": "out",
  "server": {
    "url": "http://192.168.1.217:3001",
    "cleartext": true
  },
  "ios": {
    "contentInset": "automatic",
    "scrollEnabled": true
  },
  "android": {
    "allowMixedContent": true
  },
  "plugins": {
